
@{
    ViewData["Title"] = "Dashboard";
}
 
<script>
    function preventBack() {
        window.history.forward();
    }
    setTimeout("preventBack()", 0);
    window.onunload = function () { null; }

     
    if (window.history.replaceState) {
        window.history.replaceState(null, null, window.location.href);
    }
  </script>
<nav class="navbar navbar-inverse">
    <div class="container-fluid">
        <div class="navbar-header">
            <a class="navbar-brand" href="#">eTill Remote</a>
        </div>
        @if (ViewBag.Pass == "Show")
        {
            <ul class="nav navbar-nav">
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        Menu
                        <span class="caret"></span>
                    </a>
                    <ul class="dropdown-menu">
                        <li>@Html.ActionLink("Stock", "StockView", "Home")</li>
                        <li><a href="#">Sales</a></li>
                        <li><a href="#">Debtors</a></li>
                    </ul>
                </li>
            </ul>
        }
        else if (ViewBag.Pass == "Error")
        {
            <h4>Login Failed for User  @ViewBag.ConnectingUser</h4>
        }
        else
        {

        }
    </div>
</nav>

<h1>Welcome to Dashboard</h1>

@Html.ActionLink("SignOut", "SignOut")

<form action="~/Login/Dashboard" method="post">
    @Html.DropDownList("IPAddress")
    <input type="submit" value="submit" />
</form>